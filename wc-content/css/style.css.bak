/*slider css start*/
#slider{
	box-sizing: border-box;
	background-position: center top;
	overflow: hidden;
	white-space: nowrap;
}
#sliderBox #slideContainer{
	height: 100%;
	position: relative;
    white-space: nowrap;
    font-size: 0px;
    -webkit-transition:all 0.4s ease-in-out;
    -moz-transition:all 0.4s ease-in-out;
    -ms-transition:all 0.4s ease-in-out;
    -o-transition:all 0.4s ease-in-out;
    transition:all 0.4s ease-in-out;
}
#sliderBox #slideContainer .slide{
    display: inline-block;
    height: 100%;
    width: 100%;
    vertical-align: text-top;
    text-align: right;
    position: relative;	
}
#sliderBox .slide .content{
	font-size: initial;
	width: 50%;
	min-width: 480px;
	float: right;
	position: absolute;
	text-align: center;
	white-space: initial;
	padding: 20px;
	color:white;
	display: flex;	
	justify-content: center;
	align-content: center;
	align-items: center;
	flex-direction: column;
	top: 0px;
	/*left: 0px;*/
	right: 8%;
	bottom: 0px;
	margin: auto;
	font-family: "TRAJANPRO";
}
#sliderBox .slide .content.extranrt{
	float: right !important;
	right: 2%;/*updated from right: 8% to 2%;*/
	left: unset;
}
#sliderBox .slide .content.fllf{
	float: left !important;
	left: 6%;
	right: unset;
}
#sliderBox .slide .content.sm{
	float: right !important;
	/*left: unset;*/
	right: -2%;
}
#sliderBox .slide .content.bottom{
	float: none !important;
	z-index: 999999;
	left: 0px;
	right: 0px;
	height: 100px;
	width: 70%;
	min-width: 900px;
	display: inline-block;
	bottom: 10% !important;
	margin-bottom: 5%;
	font-family: "TRAJANPRO";
	background-color: rgba(4,4,4,.4);
}
#sliderBox .slide .content h1{
	font-family: "TRAJANPRO";
	white-space: nowrap;
	font-weight: 900;
}
#sliderBox .slide .content.bottom h1{
	font-weight: 900;
	    margin: 0px;
}
#sliderBox .slide .content p{
	font-weight: 100 !important;
	font-size: 14px;
}
#sliderBox .slide .content .pin{
	display: inline-block;
	width: 30px;
	height: 30px;
	vertical-align: middle;
	background-repeat: no-repeat;
	background-position: center;
	background-size: contain;
	background-image: url(../images/icons/plansslogo.png);
}

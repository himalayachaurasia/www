/*buttons*/
.btn{
	display: inline-block;
	text-decoration: none;
	padding: 10px 20px;
	text-align: center;
}
.btn-default{
	background-color: rgb(0, 128, 64);
	padding: 10px 20px;
	color: white;
}
.btn-highlight{
	display: inline-block;
	height: auto;
	font-size: 28px;
	max-width: 460px;
	/*transform: scale(1.3);*/
	line-height: 90%;
	/*padding: 0px;*/
	padding:20px;
	font-size: 32px;
	text-align: center;
	font-family: "Calisto MT";
	text-decoration: none;
	box-sizing: border-box;
	background-color: transparent;
	border:1px solid #a68b46;
	color: #fff;
	text-decoration: none !important;
	position: relative;
	/*transform: scale(1.4);*/
}
.btn-highlight:before{
	content: "";
	display: inline-block;
	position: absolute;
	border:3px solid #a68b46;
	border-right: none;
	padding:5px 0px;
	box-sizing: unset;
	left: -1px;
	top: -2px;
	height: calc( 100% + 4px );
	box-sizing: border-box;
	width: 10px;
}
.btn-highlight:after{
	content: "";
	display: inline-block;
	position: absolute;
	border:3px solid #a68b46;
	border-left: none;
	right: -1px;
	top: -2px;
	height: calc( 100% + 4px );
	box-sizing: border-box;
	width: 10px;
}

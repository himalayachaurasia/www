	function slider(){
		tx=setInterval(function(){
			focus++;
			if(focus<0 || focus >=length){
				focus=0;
			}
//			changeSlide();
		},5000);
	}
	function changeSlide(){
			if(focus >= 0 && focus < length ){
				$("#slideContainer .slide:nth-child(1)").animate({
